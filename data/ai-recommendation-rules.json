  "adaptiveAlgorithm": {
    "feedbackLoop": {
      "checkFrequency": "weekly",
      "metrics": [
        "workout_completion_rate",
        "exercise_difficulty_rating",
        "soreness_level",
        "energy_level",
        "sleep_quality",
        "nutrition_adherence",
        "weight_change",
        "body_measurements"
      ],
      "adjustmentThresholds": {
        "completion_rate_low": {
          "threshold": "<70%",
          "action": "reduce_volume_or_difficulty"
        },
        "too_easy": {
          "threshold": "difficulty_rating < 5/10",
          "action": "increase_intensity_or_volume"
        },
        "excessive_soreness": {
          "threshold": "soreness > 7/10 for 3+ days",
          "action": "add_rest_day_or_deload"
        },
        "no_progress": {
          "threshold": "no_change_for_4_weeks",
          "action": "change_program_or_adjust_nutrition"
        }
      }
    },
    
    "autoAdjustments": {
      "progressive_overload": {
        "when": "user_completes_all_reps_for_2_consecutive_weeks",
        "action": "increase_weight_by_2.5-5% or increase_reps_by_2"
      },
      "deload": {
        "when": "every_4-6_weeks or fatigue_indicators",
        "action": "reduce_volume_by_40-50% for_1_week"
      },
      "plateau_break": {
        "when": "no_progress_for_3-4_weeks",
        "action": "change_exercise_selection or rep_ranges or training_split"
      }
    }
  },
  
